package principal;

public class DiaDosNamorados extends CartaoWeb{
	
	private String Data;
		
	public DiaDosNamorados (String destinatario, String Data) {
		setDestinatario (destinatario);
		this.Data = Data;
	}

	public String getData() {
		return Data;
	}

	public void setData(String data) {
		this.Data = data;
	}
	
	public String showMessage(String mensagem, String remetente) {
		return "Destrinatario: \n" + getDestinatario() + mensagem + "\n\n" + remetente +"\n"+ this.Data;
		  
	}
}