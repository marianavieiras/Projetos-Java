package principal;

public class Exercicio_08 {
	public static void main(String [] args) {
	
		System.out.printf("Caracter %c com o valor %d\n", 'A', ((int) 'A'));
		System.out.printf("Caracter %c com o valor %d\n", 'B', ((int) 'B'));
		System.out.printf("Caracter %c com o valor %d\n", 'C', ((int) 'C'));
		System.out.printf("Caracter %c com o valor %d\n", 'a', ((int) 'a'));
		System.out.printf("Caracter %c com o valor %d\n", 'b', ((int) 'b'));
		System.out.printf("Caracter %c com o valor %d\n\n", 'c', ((int) 'c'));
		
		System.out.printf("Caracter %c com o valor %d\n", '$', ((int) '$'));
		System.out.printf("Caracter %c com o valor %d\n\n", '*', ((int) '*'));
		
		System.out.printf("Caracter %c com o valor %d\n", '0', ((int) '0'));
		System.out.printf("Caracter %c com o valor %d\n", '1', ((int) '1'));
		System.out.printf("Caracter %c com o valor %d\n", '2', ((int) '2'));
		
	}

}
